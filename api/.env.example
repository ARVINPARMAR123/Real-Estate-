# Copy to .env and fill values
# MongoDB Atlas format:
# mongodb+srv://<USER>:<PASSWORD>@<CLUSTER_HOST>/<DB_NAME>?retryWrites=true&w=majority
# If PASSWORD has special characters (like @), URL-encode it.

DATABASE_URL="mongodb+srv://<USER>:<PASSWORD>@<CLUSTER_HOST>/<DB_NAME>?retryWrites=true&w=majority"
CLIENT_URL="http://localhost:5173"
JWT_SECRET_KEY="change-me-to-a-long-random-string"
PORT=8800
